                "operationId": "eb56a1bc1ad49b2d1090a8bf31f8a329",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "ID de la salle",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "course_type_name"
                                ],
                                "properties": {
                                    "course_type_name": {
                                        "type": "string",
                                        "maxLength": 45,
                                        "example": "Mathematics"
                                    },
                                    "active_flag": {
                                        "type": "integer",
                                        "example": 1
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Mis à jour effectuée avec succès"
                    },
                    "400": {
                        "description": "Demande invalide"
                    }
                }
            }
        },
        "/api/rooms/list": {
            "get": {
                "tags": [
                    "Rooms"
                ],
                "summary": "Liste des salles",
                "operationId": "d32929477f9fa7ac7828816c17d0c99a",
                "responses": {
                    "200": {
                        "description": "OK"
                    }
                }
            }
        },
        "/api/rooms/new": {
            "post": {
                "tags": [
                    "Rooms"
                ],
                "summary": "Ajouter une nouvelle salle",
                "operationId": "a726f9ee5a345991beaf1e89765f4455",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "name",
                                    "location",
                                    "capacity"
                                ],
                                "properties": {
                                    "room_name": {
                                        "type": "string",
                                        "maxLength": 255,
                                        "example": "Salle A"
                                    },
                                    "active_flag": {
                                        "type": "integer",
                                        "example": 1
                                    },
                                    "capacity": {
                                        "type": "integer",
                                        "example": 50
                                    },
                                    "length": {
                                        "type": "number",
                                        "format": "float",
                                        "example": 20.5
                                    },
                                    "width": {
                                        "type": "number",
                                        "format": "float",
                                        "example": 15
                                    },
                                    "height": {
                                        "type": "number",
                                        "format": "float",
                                        "example": 10
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Création avec succès"
                    },
                    "400": {
                        "description": "Demande invalide"
                    }
                }
            }
        },
        "/api/rooms/status/{id}": {
            "patch": {
                "tags": [
                    "Rooms"
                ],
                "summary": "Activer/Désactiver une salle",
                "operationId": "6bb8b0a74b612cf709da98f4d5fd5f28",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "ID de la salle",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Réussite"
                    },
                    "404": {
                        "description": "Introuvable"
                    }
                }
            }
        },
        "/api/rooms/update/{id}": {
            "put": {
                "tags": [
                    "Rooms"
                ],
                "summary": "Mettre les informations relatives à une salle",
                "operationId": "017102629a3e515381f7b470ea93e881",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "ID de la salle",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "room_name": {
                                        "type": "string",
                                        "maxLength": 255,
                                        "example": "Salle A"
                                    },
                                    "active_flag": {
                                        "type": "integer",
                                        "example": 1
                                    },
                                    "capacity": {
                                        "type": "integer",
                                        "example": 50
                                    },
                                    "length": {
                                        "type": "number",
                                        "format": "float",
                                        "example": 20.5
                                    },
                                    "width": {
                                        "type": "number",
                                        "format": "float",
                                        "example": 15
                                    },
                                    "height": {
                                        "type": "number",
                                        "format": "float",
                                        "example": 10
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Mise à jour réussie"
                    },
                    "404": {
                        "description": "Salle introuvable"
                    }
                }
            }
